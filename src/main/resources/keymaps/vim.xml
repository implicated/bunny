<keymap version="1" name="vim" parent="Mac OS X 10.5+" disable-mnemonics="true">
  <!--VCS-->
  <action id="CheckinProject" />
  <action id="Diff.ApplyNonConflicts">
    <keyboard-shortcut first-keystroke="alt p" />
  </action>
  <action id="NextDiff">
    <keyboard-shortcut first-keystroke="alt j" />
  </action>
  <action id="PreviousDiff">
    <keyboard-shortcut first-keystroke="alt k" />
  </action>
  <action id="Diff.ApplyLeftSide">
    <keyboard-shortcut first-keystroke="alt h" />
  </action>
  <action id="Diff.ApplyRightSide">
    <keyboard-shortcut first-keystroke="alt l" />
  </action>
  <action id="Diff.IgnoreLeftSide">
    <keyboard-shortcut first-keystroke="shift alt h" />
  </action>
  <action id="Diff.IgnoreRightSide">
    <keyboard-shortcut first-keystroke="shift alt l" />
  </action>
  <action id="Git.Branches">
    <keyboard-shortcut first-keystroke="shift meta j" />
  </action>
  <action id="Vcs.Push">
    <keyboard-shortcut first-keystroke="shift meta k" />
  </action>


  <!--System-->
  <action id="HideAllWindows">
    <keyboard-shortcut first-keystroke="shift meta space" />
  </action>
  <action id="GotoAction">
    <keyboard-shortcut first-keystroke="shift meta p" />
  </action>
  <action id="LeaderAction">
    <keyboard-shortcut first-keystroke="meta m" />
  </action>
  <action id="NextTab" />
  <action id="PreviousTab" />
  <action id="NextWindow" />
  <action id="PreviousWindow" />
  <action id="MinimizeCurrentWindow" />
  <action id="Editor LRU" />
  <action id="Run" />
  <action id="Debug" />
</keymap>